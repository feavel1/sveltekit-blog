	--color-warning-300: 243 241 181; /* ⬅ #f3f1b5 */
	--color-warning-400: 235 231 126; /* ⬅ #ebe77e */
	--color-warning-500: 226 221 71; /* ⬅ #e2dd47 */
	--color-warning-600: 203 199 64; /* ⬅ #cbc740 */
	--color-warning-700: 170 166 53; /* ⬅ #aaa635 */
	--color-warning-800: 136 133 43; /* ⬅ #88852b */
	--color-warning-900: 111 108 35; /* ⬅ #6f6c23 */
	/* error | #ff0000 */
	--color-error-50: 255 217 217; /* ⬅ #ffd9d9 */
	--color-error-100: 255 204 204; /* ⬅ #ffcccc */
	--color-error-200: 255 191 191; /* ⬅ #ffbfbf */
	--color-error-300: 255 153 153; /* ⬅ #ff9999 */
	--color-error-400: 255 77 77; /* ⬅ #ff4d4d */
	--color-error-500: 255 0 0; /* ⬅ #ff0000 */
	--color-error-600: 230 0 0; /* ⬅ #e60000 */
	--color-error-700: 191 0 0; /* ⬅ #bf0000 */
